{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.10",
		"laravelcollective/annotations": "5.1.*",
		"pragmarx/tracker": "^1.0",
		"pragmarx/firewall": "^0.5.2",
		"geoip/geoip": "~1.14",
		"guzzlehttp/guzzle": "~5.3|~6.0",
		"mailgun/mailgun-php": "~1.7.2",
		"rowland/ooglee-core": "dev-master",
		"rowland/ooglee-blog": "dev-master",
		"rowland/ooglee-user": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"laracasts/generators": "^1.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"Ooglee\\": "src/OoGlee"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},

	"minimum-stability": "dev",
	"prefer-stable" : true
}
